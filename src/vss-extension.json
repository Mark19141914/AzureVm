{
    "manifestVersion": 1,
    "id": "AzureVirtualMachineManagerTask",
    "name": "Azure Virtual Machine Manager Task",
    "version": "1.0.2",
    "publisher": "vfabing",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Tools for managing Azure Virtual Machine",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/128px-Computer.svg.png"
    },
    "tags": [
        "Azure",
        "Resource Manager",
        "Virtual Machine",
        "Start"
    ],
    "screenshots": [
        {
            "path": "images/Configure-Azure-VM-Start.png"
