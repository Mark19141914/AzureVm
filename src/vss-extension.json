        },
        "license": {
            "path": "LICENSE.txt"
        }
    },
    "links": {
        "getstarted": {
            "uri": "https://github.com/vfabing/AzureVm/blob/master/src/overview.md"
        },
        "support": {
            "uri": "https://github.com/vfabing/AzureVm/issues"
        },
        "repository": {
            "uri": "https://github.com/vfabing/AzureVm"
        },
        "issues": {
            "uri": "https://github.com/vfabing/AzureVm/issues"
        }
    },
    "files": [
        {
            "path": "azVmManagerTask"
        },
        {
            "path": "images",
            "addressable": true
        }
